package ex03_number;

public class Ex03 {

	public static void main(String[] args) {
		
		// 대입연산
		int score = 100; // "=" -> 대입연산
		System.out.println(score);
		
		//연습
		// x에 10이 있고, ㅛ에 20이있다.
		//x와 y에 저장된 값을 서로 교환하시오
		int x = 10;
		int y = 20;
		int temp;
		
		temp = x;
		x = y;
		y = temp;
		System.out.println(x);
		System.out.println(y);

	}

}
