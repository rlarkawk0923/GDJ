package ex01_branch;

public class EX01_if {

	public static void main(String[] args) {
		
		//if문
		//조건을 만족하는 경우에만 실행
		// if(조건) {
		//            실행문
		//}
		
		int score = 100;
		
		if(score >=60) {
			System.out.println("합격");
		}
		
		if(score < 60 ){
			System.out.println("불합격");
		}

	}

}
